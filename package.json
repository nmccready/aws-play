  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmccready/aws-play.git"
  },
  "license": "MIT",
  "author": "",
  "bin": {
    "awsKmsDecrypt": "aws/kms/decrypt/cli.js",
    "awsKmsEncrypt": "aws/kms/encrypt/cli.js"
  },
  "files": [
    "aws/**/*",
    "debug.js"
  ],
  "scripts": {
    "lint": "npm run lint:go && npm run lint:js",
