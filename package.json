  "overrides": {
    "fast-xml-parser": "5.3.6"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.400.0",
    "@aws-sdk/credential-providers": "^3.400.0",
    "aws-sdk-v3-proxy": "2.2.0",
    "debug-fabulous": "^2.0.1",
    "through2": "^4.0.2",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@znemz/aws-play": "file:./",
    "commit-and-tag-version": "12",
    "eslint": "10.0.2",
    "globals": "^17.3.0",
    "sort-package-json": "3.6"
  }
}
